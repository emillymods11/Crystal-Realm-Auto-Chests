-- Crear ScreenGui y ImageLabel
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

local imageLabel = Instance.new("ImageLabel")
imageLabel.Parent = screenGui
imageLabel.Size = UDim2.new(0.5, 0, 0.5, 0) -- Ajusta el tamaño según lo necesario
imageLabel.Position = UDim2.new(0.25, 0, 0.25, 0) -- Ajusta la posición según lo necesario
imageLabel.Image = "rbxassetid://116416554968553" -- Reemplaza con el ID de tu imagen

-- Esperar 3 segundos antes de ejecutar el segundo script
wait(3)

-- Ejecutar el segundo script
loadstring(game:HttpGet("https://raw.githubusercontent.com/markxd07/get-chest-bf/main/luxuryhub.lua"))()
